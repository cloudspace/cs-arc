{
  "name": "cs-arc",
  "version": "0.0.1",
  "dependencies": {
    "d3": "~3.5.0"
  },
  "scripts": {
    "vanilla":     "browserify src/arc.js > arc-vanilla.js",
    "angular-1-x": "browserify src/connectors/arc-angular-1-x.js > arc-angular.js",
    "react-0-x":   "browserify src/connectors/arc-react-0-x.js -t reactify> arc-react.js",
    "compile":     "npm run-script vanilla && npm run-script angular-1-x && npm run-script react-0-x"
  },
  "devDependencies": {
    "browserify": "10.1.0",
    "reactify": "1.1.1",
    "karma": "^0.12.16",
    "karma-phantomjs-launcher": "^0.1.4",
    "karma-jasmine": "^0.3.0",
    "karma-ng-html2js-preprocessor": "^0.1.0",
    "phantomjs": "^1.9.15"
  }
}